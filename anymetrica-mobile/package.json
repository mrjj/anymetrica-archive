{
  "name": "anymetrica-mobile",
  "version": "0.2.0",
  "main": "node_modules/expo/AppEntry.js",
  "author": {
    "name": "Ilya Kutukov",
    "email": "post.ilya@gmail.com",
  },
  "scripts": {
    "clean:all": "npm run clean && npm run clean:locks && npm run clean:libs",
    "clean": "npm cache clean --force && if [[ $(command -v yarn) ]] ; then yarn cache clean ; fi",
    "clean:locks": "rm -f ./package-lock.json && rm -f ./yarn.lock",
    "clean:libs": "rm -rf ./node_modules",
    "bootstrap": "sudo npm install -g create-react-native-app && sudo npm install -g metro",
    "avd:list": "`~/Library/Android/sdk/tools/emulator -list-avds`",
    "avd:create": "~/Library/Android/sdk/tools/bin/avdmanager create avd -n anymetrica_nexus_5x_api_27 -k \"system-images;android-27;google_apis;x86\"",
    "avd:run": "~/Library/Android/sdk/tools/emulator -avd `~/Library/Android/sdk/tools/emulator -list-avds`",
    "start:android": "expo start --android",
    "start:ios": "expo start --ios",
    "eject": "expo eject",
    "build:android": "expo build:android",
    "build:ios": "expo build:ios",
    "test": "node ./node_modules/jest/bin/jest.js --watchAll",
    "commented_preinstall": "npm install -g expo-cli"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@grpc/proto-loader": "0.3.0",
    "grpc": "1.18.0",
    "react-test-renderer": "16.6.3",
    "antd-mobile-rn": "2.3.1",
    "expo": "31.0.5",
    "react": "16.5.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-31.0.1.tar.gz",
    "anymetrica-api": "../anymetrica-api",
    "react-native-sound-recorder": "1.3.2",
    "react-navigation": "2.18.2"
  },
  "devDependencies": {
    "babel-preset-expo": "5.0.0",
    "babel-plugin-import": "1.11.0",
    "jest-expo": "31.0.0",
    "sentry-expo": "1.10.0",
    "flow-bin": "0.86.0",
    "react-native-scripts": "^1.5.0",
    "mocha": "5.2.0"
  }
}
