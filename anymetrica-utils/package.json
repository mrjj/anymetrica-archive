{
  "name": "anymetrica-utils",
  "description": "AnyMetrica JavaScript utils for Node.js and browsers",
  "version": "1.7.8",
  "main": "./lib/index-web.js",
  "module": "./lib/index.js",
  "browser": "dist/anymetrica-utils.js",
  "license": "LICENCE.md",
  "author": {
    "name": "Ilya Kutukov",
    "email": "post.ilya@gmail.com"
  },
  "files": [
    "src/",
    "dist/",
    "lib/",
    "!__mocks__",
    "!__tests__"
  ],
  "scripts": {
    "clean": "npm run build:clean && npm run clean:cache",
    "clean:all": "npm run build:clean && npm run clean:cache && npm run clean:locks && npm run clean:libs",
    "clean:cache": "npm cache clean --force && if [[ $(command -v yarn) ]] ; then yarn cache clean ; fi",
    "clean:locks": "rm -f ./package-lock.json && rm -f ./yarn.lock",
    "clean:libs": "rm -rf ./node_modules",
    "build:flow": "flow-copy-source -i '**/__tests__/**' ./src/ ./lib/",
    "build:babel": "babel ./src --ignore '**/__tests__/**' --out-dir ./lib",
    "build:clean": "rm -rf ./dist && rm -rf ./lib",
    "build:webpack": "webpack --ignore  '**/__tests__/**' --config ./webpack.config.js --out-dir ./dist",
    "build": "npm run build:clean && npm run build:webpack && npm run build:babel && npm run build:flow",
    "prepublishOnly": "npm run build",
    "build:watch": "webpack --config ./webpack.config.js --watch",
    "test": "npm run test:jest && npm run test:import",
    "test:jest": "jest ./src --coverage",
    "test:watch": "jest ./src --watch --notify",
    "test:import": "node ./src/__tests__/testNodeImport.js",
    "test:nocover": "jest ./src",
    "flow": "flow check ./src",
    "lint": "eslint ./src",
    "lint:fix": "eslint . --fix",
    "ci": "npm install && npm run build && npm run test && npm run lint && npm run flow"
  },
  "dependencies": {
    "babel-polyfill": "6.26.0",
    "browserify": "16.2.3",
    "execa": "1.0.0",
    "js-yaml": "3.12.1",
    "json-stringify-safe": "5.0.1",
    "lodash.clone": "4.5.0",
    "lodash.clonedeep": "4.5.0",
    "lodash.compact": "3.0.1",
    "lodash.defaults": "4.2.0",
    "lodash.difference": "4.5.0",
    "lodash.flatten": "4.4.0",
    "lodash.flattendeep": "4.4.0",
    "lodash.foreach": "4.5.0",
    "lodash.frompairs": "4.0.1",
    "lodash.get": "4.4.2",
    "lodash.includes": "4.3.0",
    "lodash.intersection": "4.4.0",
    "lodash.map": "4.6.0",
    "lodash.omit": "4.5.0",
    "lodash.pick": "4.4.0",
    "lodash.range": "^3.2.0",
    "lodash.set": "4.3.2",
    "lodash.slice": "4.2.0",
    "lodash.sortby": "4.7.0",
    "lodash.union": "4.6.0",
    "lodash.uniq": "4.5.0",
    "mkdirp": "0.5.1",
    "moment": "2.24.0",
    "parse-json": "4.0.0",
    "request": "2.88.0",
    "traverse": "0.6.6"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "10.0.1",
    "babel-jest": "23.6.0",
    "babel-plugin-external-helpers": "6.22.0",
    "babel-plugin-transform-async-to-generator": "6.24.1",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-decorators-legacy": "1.3.5",
    "babel-plugin-transform-flow-comments": "6.22.0",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "1.7.0",
    "eslint": "5.12.1",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-plugin-flowtype": "2.50.3",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jest": "22.1.3",
    "flow-bin": "0.91.0",
    "flow-copy-source": "2.0.2",
    "jest": "23.6.0",
    "remove-flow-types-loader": "1.1.0",
    "webpack": "4.29.0",
    "webpack-cli": "3.2.1",
    "webpack-node-externals": "1.7.2"
  }
}
