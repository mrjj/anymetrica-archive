  "scripts": {
    "start:prod": "BROWSER=none NODE_ENV=production react-scripts start",
    "dev": "BROWSER=none NODE_ENV=development PORT=3000 react-scripts start",
    "start": "npm run start:prod",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "coverage": "npm test -- --coverage",
    "clean": "npm run clean:cache ; npm run clean:locks ; rm -rf ./node_modules ; rm -rf ./flow-typed",
    "clean:cache": "npm cache clean --force && if [[ $(command -v yarn) ]] ; then yarn cache clean ; fi",
    "clean:node_modules": "find . -type d -name \"node_modules\" -exec rm -rf \"{}\" \\;",
    "clean:locks": "find . -name 'npm.lock' -delete && find . -name 'package-lock.json' -delete",
    "test:watch": "jest ./src --watch --notify",
    "test:nocover": "jest ./src",
    "flow": "flow check ./src",
    "lint": "eslint ./src",
    "lint:fix": "eslint . --fix",
    "postinstall": "npm run build",
    "ci": "npm run build && npm run lint && npm run flow"
  },
  "files": [
    "src/"
  ],
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">2.0%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
